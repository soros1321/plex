    "html-webpack-plugin": "2.29.0",
    "identicon.js": "^2.3.1",
    "jest": "20.0.4",
    "lodash.compact": "^3.0.1",
    "moment": "^2.22.0",
    "ngrok": "^2.2.26",
    "object-assign": "4.1.1",
    "postcss-flexbugs-fixes": "3.2.0",
